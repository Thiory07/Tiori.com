<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-NMLD7KX"
height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->
{{!-- recursive loop through body content tags--}}
{{#ifObject this.content}}
  <{{this.tag}}>{{{this.content}}}</{{this.tag}}>
{{else}}
  <{{this.tag}}>
    {{#each this.content}}
      {{>bodyContent}}
    {{/each}}
  </{{this.tag}}>
{{/ifObject}}
